<div class="form-area">
  <div  id="subscriber_form_area">
    <p class="email">
      <label for="subscriber_email">Email:</label>
      <%= f.text_field :email %>
    </p>
    <p class="password">
      <label for="subscriber_password">Password:</label>
      <%= f.password_field :password %>
    </p>
    <p class="status">
      <label for="subscriber_status">Status:</label>
      <%= @subscriber.status %>
    </p>
  </div>
</div>

<p class="buttons">
  <%= save_model_button(@subscriber) %> or
  <%= link_to "cancel", subscriber_index_path %>
</p>